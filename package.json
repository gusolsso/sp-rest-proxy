{
  "name": "sp-rest-proxy",
  "description": "SharePoint REST API Proxy for Node.js and Express local serve",
  "version": "2.10.2",
  "main": "./dist/index.js",
  "typings": "./dist/index/RestProxy",
  "scripts": {
    "prepare": "npm run build",
    "serve": "ts-node ./src/server",
    "build": "npm run lint && npm run clean && npm run tsc",
    "lint": "tslint -p .",
    "tsc": "tsc -p .",
    "clean": "rimraf ./dist",
    "test": "ts-node ./test/init && mocha --opts test/mocha.opts || ECHO.",
    "test:manual": "ts-node ./test/manual/server",
    "test:ci": "mocha --opts test/mocha.opts --ci || ECHO.",
    "docker": "ts-node ./docker/scripts/publish"
  },
  "keywords": [
    "sharepoint",
    "rest",
    "proxy",
    "debug",
    "API"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koltyakov/sp-rest-proxy.git"
  },
  "author": "Andrew Koltyakov <andrew.koltyakov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koltyakov/sp-rest-proxy/issues"
  },
  "homepage": "https://github.com/koltyakov/sp-rest-proxy#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "sp-auth": "git+ssh://git@git.haxakon.se:2222/fmv/sp-auth.git",
    "sp-auth-config": "git+ssh://git@git.haxakon.se:2222/fmv/sp-auth-config.git",
    "socket.io": "^2.3.0",
    "sp-request": "^2.1.3"
  },
  "devDependencies": {
    "@pnp/pnpjs": "^1.3.7",
    "@types/chai": "^4.2.5",
    "@types/core-js": "^2.5.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.14",
    "@types/request-promise": "^4.1.45",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/xml2js": "^0.4.5",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "cpass": "^2.2.0",
    "dotenv": "^8.2.0",
    "minimist": "^1.2.0",
    "mocha": "^6.2.2",
    "rimraf": "^3.0.0",
    "sp-pnp-node": "^2.2.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.2",
    "xml2js": "^0.4.22"
  }
}
